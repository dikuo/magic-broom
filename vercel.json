{
  "version": 2,
  "name": "magicBroom-app", 
  
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "npm run build", 
        "outputDirectory": "dist",
        "installCommand": "npm install"
      }
    }
  ],
  
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ]
}